// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "files.associations": {
    "*.vue": "vue"
  },
  "files.autoSave": "off",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "vue",
      "autoFix": true
    }
  ],
  "eslint.options": {
    "plugins": [
      "html"
    ]
  },
  "eslint.autoFixOnSave": true,
  "eslint.enable": true,
  "editor.detectIndentation": false
}